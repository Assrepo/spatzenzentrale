{
  "name": "database-viewer",
  "version": "1.0.0",
  "type": "core-feature",
  "description": "Database administration interface for viewing tables and data",
  "route": "/api/db-admin",
  "enabled": true,
  "env": {
    "DB_ADMIN_MAX_ROWS": "100"
  },
  "permissions": [
    "database:read",
    "database:admin"
  ],
  "frontend": {
    "type": "svelte-component",
    "entry": "./frontend/dist/plugin-bundle.js",
    "style": "./frontend/dist/plugin-bundle.css",
    "name": "Database Admin",
    "description": "Datenbank-Verwaltungsinterface zum Anzeigen und Durchsuchen der Tabellen",
    "icon": "🗄️",
    "route": "/plugin/database-viewer",
    "color": "bg-green-500 hover:bg-green-600",
    "category": "system",
    "priority": 5
  }
}